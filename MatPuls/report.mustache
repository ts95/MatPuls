<div class="matpuls-report">
  <header>
    <img class="logo" alt="MatPuls Logo" src="data:image/png;base64," width="100px">
    <div>
      <div class="title">MatPuls rapport</div>
      <div class="customer">{{ customer }}</div>
      <div class="date"><span class="muted">Dato:&nbsp;</span>{{ date }}</div>
    </div>
  </header>
  <content>
    {{#coolers}}
    <div class="cooler">
      <h3 class="title">{{ name }} ({{ tempRange }})</h3>
      <table>
        <thead>
          <tr>
            <th class="unit">Enhet</th>
            <th class="placeMeasured">Sted m√•lt</th>
            <th class="temp">Temperatur</th>
          </tr>
        </thead>
        <tbody>
          {{#items}}
          <tr>
            <td>{{ unit }}</td>
            <td>{{ placeMeasured }}</td>
            <td>{{ temp }}</td>
          </tr>
          {{/items}}
        </tbody>
      </table>
    </div>
    {{/coolers}}
  </content>
</div>

<style type="text/css">
  body {
    margin: 0;
    padding: 0;
    font-family: sans-serif;
    background-color: #fafafa;
  }

  * {
    box-sizing: border-box;
  }

  .matpuls-report {
    display: flex;
    flex-wrap: wrap;
    padding: 10px;
  }

  .matpuls-report .muted {
    color: #555;
  }

  .matpuls-report header {
    width: 100%;
    flex-direction: row;
    padding: 10px;
    border-bottom: 1px solid #ddd;
  }

  .matpuls-report header div {
    display: inline-block;
    vertical-align: top;
    padding: 5px;
    margin: 5px;
  }

  .matpuls-report header div * {
    display: flex;
    padding: 0 0 5px 0;
  }

  .matpuls-report header div .title {
    font-weight: bold;
  }

  .matpuls-report content {
    padding: 10px;
  }

  .matpuls-report content .cooler .title {
    font-size: 1rem;
    font-weight: bold;
    margin: 0;
    padding: 10px 5px 0 5px;
  }

  .matpuls-report content .cooler table {
    background-color: white;
    margin: 10px 10px 10px 0;
    border-collapse: collapse;
  }

  .matpuls-report content .cooler table .unit, .placeMeasured {
    min-width: 300px;
  }

  .matpuls-report content .cooler table th, td {
    padding: 10px;
    text-align: left;
    border: 1px solid black;
  }
</style>
